[
  {
    "id": "vidcheck:server:public:anonymous",
    "upstream": {
      "preserve_host": true,
      "url": "http://vidcheck-server:8000",
      "strip_path": ".factly/vidcheck/server"
    },
    "match": {
      "url": "http://127.0.0.1:4455/.factly/vidcheck/server/<(videos|ratings)>/embed<(/\\d+|)(/|)>",
      "methods": [
        "GET"
      ]
    },
    "authenticators": [
      {
        "handler": "anonymous"
      }
    ],
    "authorizer": {
      "handler": "allow"
    },
    "mutators": [
      {
        "handler": "noop"
      }
    ]
  },
  {
    "id": "vidcheck:server:protected",
    "upstream": {
      "preserve_host": true,
      "url": "http://vidcheck-server:8000",
      "strip_path": ".factly/vidcheck/server"
    },
    "match": {
      "url": "http://127.0.0.1:4455/.factly/vidcheck/server/<(videos|media|ratings|ratings/default|claimants|spaces|permissions(/organisations|/spaces)(|/my)|requests(/organisations|/spaces)(|/my))(|/\\d+)(|/)>",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    },
    "authenticators": [
      {
        "handler": "cookie_session"
      }
    ],
    "authorizer": {
      "handler": "allow"
    },
    "mutators": [
      {
        "handler": "id_token"
      },
      {
        "handler": "hydrator"
      },
      {
        "handler": "header"
      }
    ]
  }
]