[ 
  {
    "id": "vid-check:web:anonymous",
    "upstream": {
      "preserve_host": true,
      "url": "http://vid-check-web:3000"
    },
    "match": {
      "url": "http://127.0.0.1:4455/.factly/vid-check/web/<.*>",
      "methods": [
        "GET"
      ]
    },
    "authenticators": [
      {
        "handler": "anonymous"
      }
    ],
    "authorizer": {
      "handler": "allow"
    },
    "mutators": [
      {
        "handler": "noop"
      }
    ]
  }
]