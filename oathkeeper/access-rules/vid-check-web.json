[
  {
    "id": "vid-check:web:protected",
    "upstream": {
      "preserve_host": true,
      "url": "http://vid-check-web:3000"
    },
    "match": {
      "url": "http://127.0.0.1:4455/.factly/vid-check/web/<.*>",
      "methods": [
        "GET"
      ]
    },
    "authenticators": [
      {
        "handler": "cookie_session"
      }
    ],
    "authorizer": {
      "handler": "allow"
    },
    "mutators": [
      {
        "handler": "id_token"
      }
    ],
    "errors": [
      {
        "handler": "redirect",
        "config": {
          "to": "http://127.0.0.1:4455/.factly/kavach/web/auth/login"
        }
      }
    ]
  }
]